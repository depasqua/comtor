<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
	<display-name>COMTOR KeyGen web app</display-name>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

	<servlet>
		<description>Servlet to create a COMTOR API Key</description>
		<servlet-name>KeyGenServlet</servlet-name>
		<servlet-class>org.comtor.keygen.KeyGen</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>KeyGenServlet</servlet-name>
		<url-pattern>/keygen</url-pattern>
	</servlet-mapping>
</web-app>